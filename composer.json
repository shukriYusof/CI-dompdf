{
    "name": "skytrex/ci-dompdf",
    "type": "library",
    "description": "This package for all codeigniter user who is using composer to install dependecies dompdf",
    "keywords": ["composer","codeigniter","dompdf","pdf"],
    "homepage": "https://www.sekolahcode.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Shukri Yusof",
            "email": "shuk.yusof@gmail.com",
            "homepage": "http://www.travelover.live/",
            "role": "Author"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Dompdf\\" : "src/"
        },
        "classmap" : ["lib/"]
    },
    "require": {
        "php": ">=5.4.0",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "phenx/php-font-lib": "0.5.*",
        "phenx/php-svg-lib": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.5|^6.5",
        "squizlabs/php_codesniffer": "2.*"
    }
}